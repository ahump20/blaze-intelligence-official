{
  "name": "blaze-intelligence",
  "version": "2.0.0",
  "description": "Professional-grade sports analytics platform with real-time performance tracking and AI-powered insights",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "test": "jest --testPathIgnorePatterns=/austin-portfolio-deploy/",
    "test:watch": "jest --watch --testPathIgnorePatterns=/austin-portfolio-deploy/",
    "test:coverage": "jest --coverage --testPathIgnorePatterns=/austin-portfolio-deploy/"
  },
  "keywords": [
    "sports",
    "analytics",
    "championship",
    "ai",
    "machine-learning",
    "performance"
  ],
  "author": "Austin Humphrey",
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.7.0",
    "wrangler": "^4.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.min.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/austin-portfolio-deploy/",
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/austin-portfolio-deploy/",
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "directories": {
    "test": "tests"
  },
  "bugs": {
    "url": "https://github.com/austinhumphrey/blaze-intelligence-website/issues"
  },
  "homepage": "https://github.com/austinhumphrey/blaze-intelligence-website#readme",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.61.0",
    "@balldontlie/sdk": "^1.2.2",
    "@notionhq/client": "^4.0.2",
    "@sendgrid/mail": "^8.1.5",
    "aos": "^2.3.4",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "openai": "^4.73.1",
    "pg": "^8.13.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "stripe": "^17.3.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "three": "^0.180.0",
    "typescript": "^5.9.2",
    "uuid": "^11.0.2",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.0"
  }
}
