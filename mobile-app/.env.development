# Blaze Intelligence Mobile App - Development Environment
REACT_APP_ENV=development
REACT_APP_VERSION=1.0.0
REACT_APP_BUILD_NUMBER=20250110001

# API Configuration
REACT_APP_API_URL=https://dev-api.blaze-intelligence.com
REACT_APP_WS_URL=wss://dev-ws.blaze-intelligence.com

# Authentication
REACT_APP_AUTH_DOMAIN=dev-auth.blaze-intelligence.com
REACT_APP_JWT_SECRET=dev_jwt_secret_key_2025

# Video Analysis
REACT_APP_MAX_VIDEO_SIZE=100000000
REACT_APP_SUPPORTED_FORMATS=mp4,mov,avi
REACT_APP_ANALYSIS_TIMEOUT=300000

# Sports Data APIs
REACT_APP_MLB_API_KEY=dev_mlb_api_key
REACT_APP_NFL_API_KEY=dev_nfl_api_key
REACT_APP_NBA_API_KEY=dev_nba_api_key
REACT_APP_NCAA_API_KEY=dev_ncaa_api_key

# Firebase (for push notifications)
REACT_APP_FIREBASE_API_KEY=dev_firebase_api_key
REACT_APP_FIREBASE_PROJECT_ID=blaze-intelligence-dev
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=123456789

# AWS S3 (for video storage)
REACT_APP_S3_BUCKET=blaze-intelligence-dev-videos
REACT_APP_S3_REGION=us-east-1

# Analytics
REACT_APP_MIXPANEL_TOKEN=dev_mixpanel_token
REACT_APP_GA_TRACKING_ID=GA-DEV-BLAZE

# Feature Flags
REACT_APP_ENABLE_BETA_FEATURES=true
REACT_APP_ENABLE_DEBUG_MODE=true
REACT_APP_ENABLE_MOCK_DATA=true

# Logging
REACT_APP_LOG_LEVEL=debug
REACT_APP_SENTRY_DSN=https://dev-sentry-dsn@sentry.io/project