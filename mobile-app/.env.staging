# Blaze Intelligence Mobile App - Staging Environment
REACT_APP_ENV=staging
REACT_APP_VERSION=1.0.0
REACT_APP_BUILD_NUMBER=20250110002

# API Configuration
REACT_APP_API_URL=https://staging-api.blaze-intelligence.com
REACT_APP_WS_URL=wss://staging-ws.blaze-intelligence.com

# Authentication
REACT_APP_AUTH_DOMAIN=staging-auth.blaze-intelligence.com
REACT_APP_JWT_SECRET=staging_jwt_secret_key_2025

# Video Analysis
REACT_APP_MAX_VIDEO_SIZE=100000000
REACT_APP_SUPPORTED_FORMATS=mp4,mov,avi
REACT_APP_ANALYSIS_TIMEOUT=300000

# Sports Data APIs
REACT_APP_MLB_API_KEY=staging_mlb_api_key
REACT_APP_NFL_API_KEY=staging_nfl_api_key
REACT_APP_NBA_API_KEY=staging_nba_api_key
REACT_APP_NCAA_API_KEY=staging_ncaa_api_key

# Firebase (for push notifications)
REACT_APP_FIREBASE_API_KEY=staging_firebase_api_key
REACT_APP_FIREBASE_PROJECT_ID=blaze-intelligence-staging
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=987654321

# AWS S3 (for video storage)
REACT_APP_S3_BUCKET=blaze-intelligence-staging-videos
REACT_APP_S3_REGION=us-east-1

# Analytics
REACT_APP_MIXPANEL_TOKEN=staging_mixpanel_token
REACT_APP_GA_TRACKING_ID=GA-STAGING-BLAZE

# Feature Flags
REACT_APP_ENABLE_BETA_FEATURES=true
REACT_APP_ENABLE_DEBUG_MODE=false
REACT_APP_ENABLE_MOCK_DATA=false

# Logging
REACT_APP_LOG_LEVEL=info
REACT_APP_SENTRY_DSN=https://staging-sentry-dsn@sentry.io/project