# Blaze Intelligence Mobile App - Production Environment
REACT_APP_ENV=production
REACT_APP_VERSION=1.0.0
REACT_APP_BUILD_NUMBER=20250110003

# API Configuration
REACT_APP_API_URL=https://api.blaze-intelligence.com
REACT_APP_WS_URL=wss://ws.blaze-intelligence.com

# Authentication
REACT_APP_AUTH_DOMAIN=auth.blaze-intelligence.com
REACT_APP_JWT_SECRET=production_jwt_secret_key_2025

# Video Analysis
REACT_APP_MAX_VIDEO_SIZE=100000000
REACT_APP_SUPPORTED_FORMATS=mp4,mov,avi
REACT_APP_ANALYSIS_TIMEOUT=300000

# Sports Data APIs (Production keys)
REACT_APP_MLB_API_KEY=prod_mlb_api_key
REACT_APP_NFL_API_KEY=prod_nfl_api_key
REACT_APP_NBA_API_KEY=prod_nba_api_key
REACT_APP_NCAA_API_KEY=prod_ncaa_api_key

# Firebase (for push notifications)
REACT_APP_FIREBASE_API_KEY=prod_firebase_api_key
REACT_APP_FIREBASE_PROJECT_ID=blaze-intelligence-prod
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=555666777

# AWS S3 (for video storage)
REACT_APP_S3_BUCKET=blaze-intelligence-prod-videos
REACT_APP_S3_REGION=us-east-1

# Analytics
REACT_APP_MIXPANEL_TOKEN=prod_mixpanel_token
REACT_APP_GA_TRACKING_ID=GA-PROD-BLAZE

# Feature Flags
REACT_APP_ENABLE_BETA_FEATURES=false
REACT_APP_ENABLE_DEBUG_MODE=false
REACT_APP_ENABLE_MOCK_DATA=false

# Logging
REACT_APP_LOG_LEVEL=warn
REACT_APP_SENTRY_DSN=https://prod-sentry-dsn@sentry.io/project